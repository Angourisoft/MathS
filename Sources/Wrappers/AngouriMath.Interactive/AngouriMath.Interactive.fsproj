<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0-preview.4</Version>
    <Authors>WhiteBlackGoose</Authors>
    <Company>Angouri</Company>
    <Product>Wrapper of AngouriMath for Interactive</Product>
    <Description>Wrapper of AngouriMath for Interactive</Description>
    <Copyright>Angouri 2019-2020</Copyright>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/asc-community/AngouriMath</RepositoryUrl>
    <PackageTags>interactive</PackageTags>
    <PackageReleaseNotes>Dependency update</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Main.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AngouriMath.FSharp" Version="1.0.0-preview.3" />
    <PackageReference Include="Microsoft.DotNet.Interactive.Formatting" Version="1.0.0-beta.20553.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
