<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <Authors>WhiteBlackGoose</Authors>
    <Company>Angouri</Company>
    <Product>Wrapper of AngouriMath for Interactive</Product>
    <Description>Wrapper of AngouriMath for Interactive. https://am.angouri.org/#inter</Description>
    <Copyright>Angouri 2019-2020</Copyright>
    <PackageProjectUrl>https://am.angouri.org</PackageProjectUrl>
    <RepositoryUrl>https://github.com/asc-community/AngouriMath</RepositoryUrl>
    <PackageTags>interactive, fsharp, math</PackageTags>
    <PackageReleaseNotes>https://am.angouri.org/#whatsnew</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Main.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AngouriMath.FSharp" Version="1.0.0" />
    <PackageReference Include="Microsoft.DotNet.Interactive.Formatting" Version="1.0.0-beta.21108.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="5.0.1" />
  </ItemGroup>

</Project>
