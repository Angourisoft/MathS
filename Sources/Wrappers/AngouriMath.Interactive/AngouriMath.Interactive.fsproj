<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.3.0-preview.2</Version>
    <Authors>WhiteBlackGoose</Authors>
    <Company>Angouri</Company>
    <Product>Wrapper of AngouriMath for Jupyter Interactive</Product>
    <Description>Wrapper of AngouriMath for Jupyter Interactive. https://am.angouri.org/</Description>
    <Copyright>Angouri 2019-2021</Copyright>
    <PackageProjectUrl>https://am.angouri.org</PackageProjectUrl>
    <RepositoryUrl>https://github.com/asc-community/AngouriMath</RepositoryUrl>
    <PackageTags>interactive, fsharp, math</PackageTags>
    <PackageReleaseNotes>Dependency update: moved to AngouriMath.FSharp 1.3.0-preview.2
https://am.angouri.org/whatsnew</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <RepositoryType>public</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Main.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AngouriMath.FSharp" Version="1.3.0-preview.2" />
    <PackageReference Include="Microsoft.DotNet.Interactive.Formatting" Version="1.0.0-beta.21108.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="5.0.1" />
  </ItemGroup>

</Project>
